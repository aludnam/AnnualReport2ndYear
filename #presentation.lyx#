\begin_layout BeginFrame
Theoretical limits for LM - Cramer Rao bound
\end_layout

\begin_layout Itemize
Covariance matrix of an unbiased estimator 
\begin_inset Formula $\theta$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
{\bf Q}(\theta)\geq{\bf I^{-1}}(\theta)
\]

\end_inset


\end_layout

\begin_layout Itemize
Fisher information as a curvature of the log-likelihood function
\begin_inset Formula 
\[
{\bf I}_{ij}(\theta)=-\mathbb{E}\left[\frac{\partial^{2}\mathcal{L}}{\partial\theta_{i}\partial\theta_{j}}\right]
\]

\end_inset


\end_layout

\begin_layout BeginFrame
Two sources
\end_layout

\begin_layout Itemize
centers of the sources
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\theta=(c_{1},\, c_{2})
\]

\end_inset


\end_layout

\begin_layout Itemize
likelihood
\begin_inset Formula 
\[
l(\theta)=\prod_{p=1}^{N}\text{Po}(n_{p}|\lambda_{p}(\theta))
\]

\end_inset


\end_layout

\begin_layout Itemize
mean intensity image
\begin_inset Formula 
\[
\lambda(\theta)=\Lambda_{1}f_{1}(c_{1})+\Lambda_{2}f_{2}(c_{2})+b
\]

\end_inset


\begin_inset Formula 
\[
\]

\end_inset


\end_layout

\begin_layout BeginFrame
Fisher information as a cuvature of the log-likelihood
\end_layout

\begin_layout Standard

\begin_inset Formula 
\[
\mathbb{E}_{true}\left[\mathcal{L}(\theta)\right]=\sum_{p=1}^{N}\left(\lambda_{p}^{true}\log\lambda_{p}-\lambda_{p}\right)+A
\]

\end_inset


