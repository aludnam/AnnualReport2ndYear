\begin_layout Itemize
model of one source
\begin_inset Formula 
\[
s_{k}(x,t)=w_{k}(x)h_{k}(t)
\]

\end_inset


\end_layout

\begin_layout Itemize
model of an image
\begin_inset Formula 
\[
\begin{alignedat}{1}I(x,t) & =\sum_{k=1}^{N}s_{k}(x,t)\\
 & =\sum_{k=1}^{N}w_{k}(x)h_{k}(t)
\end{alignedat}
\]

\end_inset

 
\end_layout

