\begin_layout BeginFrame
Model
\end_layout

\begin_layout Itemize
matrix form - factorisation problem
\begin_inset Formula 
\[
I(x,t)=W(x)H(t)
\]

\end_inset


\end_layout

\begin_layout Itemize
relaxed model 
\begin_inset Formula 
\[
E[I(x,t)]=W(x)H(t)
\]

\end_inset


\end_layout

\begin_layout Itemize
non-negativity constraints 
\begin_inset Formula 
\[
I\geq0,\ W\geq0,\ H\geq0
\]

\end_inset


\end_layout

\begin_layout BeginFrame
Algorithms
\end_layout

\begin_layout Itemize
Maximum a posteriori fitting (MAP)
\begin_inset Newline newline
\end_inset

[Harrington et al., 2008]
\end_layout

\begin_layout Itemize
Non-negative matrix factorisation (NMF)
\begin_inset Newline newline
\end_inset

[Lee & Seung, 2001]
\end_layout

\begin_layout Itemize
Gamma - Poisson model (GaP)
\begin_inset Newline newline
\end_inset

[Canny, 2004]
\end_layout

